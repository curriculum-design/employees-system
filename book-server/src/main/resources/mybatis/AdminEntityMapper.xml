<mapper namespace="org.cdteam.employee.sys.mapper.AdminMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.cdteam.employee.sys.domain.AdminEntity">
        <id column="id" property="id" />
